{
  "name": "Vue Boilerplate",
  "version": "1.0.0",
  "description": "A Vue.js + Express Boilerplate",
  "author": "Poko <markpoko2@gmail.com>",
  "private": true,
  "scripts": {
    "build": "yarn client:build yarn server:build",
    "start:dev": "concurrently --kill-others \"yarn client:dev\" \"yarn server:start\"",
    "client:dev": "babel-node  --debug --presets es2015,stage-2 -- build/dev-server.js",
    "client:build": "node --harmony build/build.js",
    "client:unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "client:e2e": "node test/e2e/runner.js",
    "client:test": "yarn client:unit && yarn client:e2e",
    "client:lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "server:start": "gulp serve",
    "server:build": "gulp",
    "server:lint": "esw *.js server config --color",
    "server:lint:watch": "yarn lint -- --watch",
    "server:test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors --compilers js:babel-core/register ./server/**/*.test.js",
    "server:test:watch": "yarn test -- --watch",
    "server:test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors --compilers js:babel-core/register ./server/**/*.test.js",
    "server:test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "server:report-coverage": "coveralls < ./coverage/lcov.info",
    "slate": "rimraf dist && rimraf node_modules"
  },
